@using EwkQxObd.Core.Model
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "E-work IQX Onboarding";
}
@model IEnumerable<EqoContract>

<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>
                    ContractNumber
                    
                </th>
                <th>Description</th>
                <th>ValidFrom</th>
                <th>ValidTo</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a asp-action="SingleRead" asp-route-ContractID="@item.ContractNumber">
                        @item.ContractNumber
                    </a></td>
                <td>@item.Description</td>
                <td>@item.ValidFrom</td>
                <td>@item.ValidTo</td>
            </tr>
        }
    </table>
</body>
</html>