@using EwkQxObd.WebApi.Models.Serialization

@model EqoContractObject

<div class="row mb-3 mt-5">
    <div class="col-sm-1"></div>
    <h3 class="col-sm-8">Condensed Text</h3>
</div>

<div class="row mb-3">
    <div class="col-4">
        <button id="btnCopy2clipB" type="button" class="btn btn-outline-primary w-100">Copy to Clipboard</button>
    </div>
    <div class="col-sm">
        <button type="button" class="btn btn-outline-secondary w-100" disabled>work in progress</button>
    </div>
</div>

<div class="row m-0">
    <textarea class="form-control w-100" id="txaCondensedConObj" rows="24">
        @Html.Raw(Model.ToFlatText())
    </textarea>
</div>