@model Vinlks


<td>
    <a class="btn btn-outline-dark border-2 font-monospace w-100" asp-controller="ContractObject" asp-action="Detail" asp-route-Id="@Model.ContractId">@Model.ContractNumber</a>
    

</td>
<td>@Model.Description</td>
<td>
    @{
        string linkIcon = Model.NetworkId == null ? "fa-link-slash" : "fa-link";
        string btnColor = Model.NetworkId == null ? "btn-danger" : "btn-primary";
    }
    <a class="btn @btnColor font-monospace w-100">
        <span>
            
            <i class="fa-solid @linkIcon">&nbsp;@Model.NetworkId</i>@Model.SerialNumber
        </span>
    </a>
</td>
<td>@Model.InstrumentType</td>
<td>@Model.AccountNumber | @Model.OrgName</td>