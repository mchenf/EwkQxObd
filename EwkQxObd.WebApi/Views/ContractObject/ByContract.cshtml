@{
    ViewData["Title"] = "By Contract";
}

@model List<Syngio>

<h1>Contract Detail</h1>

<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>A</th>
            <th>B</th>
            <th>C</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Syngio item in @Model)
        {
            <tr>
                <td>@item.SerialNumber</td>
                <td>@item.InstrumentName</td>
            </tr>
        }
    </tbody>
</table>